{
  "name": "fastfoodfast",
  "version": "1.0.0",
  "description": "A food delivery service app for a restaurant.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register \"./server/src/test/*.spec.js\"",
    "dev": "nodemon --exec babel-node ./server/src/index.js",
    "build": "./node_modules/.bin/babel ./server/src -d build",
    "prestart": "npm run build",
    "start": "node ./build/index.js",
    "coverage" : "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/el-Joft/Fast-Food-Fast.git"
  },
  "author": "el-joft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/el-Joft/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/el-Joft/Fast-Food-Fast#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "supertest": "^3.2.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3"
  }
}
